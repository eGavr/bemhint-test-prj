{
    "levels": [
        "*.blocks"
    ],

    "excludePaths": [
        "node_modueles/**"
    ],

    "plugins": {
        "bemhint-plugins-jshint": {
            "techs": {
                "js|deps.js": true
            }
        },
        "bemhint-plugins-jscs": true,
        "bemhint-plugins-redundant-techs": {
            "techs": {
                "zip": true
            }
        },
        "bemhint-plugins-check-empty-files": true
    }
}
